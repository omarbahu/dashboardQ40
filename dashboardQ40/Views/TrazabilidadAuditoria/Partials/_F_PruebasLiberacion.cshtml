@model List<PruebaLiberacionRow>

<table style="width: 100%; border-collapse: collapse; font-size: 12px;">
    <thead>
        <tr style="background-color: #9BD2E3; font-weight: bold; text-align: center;">
            <td colspan="4" style="border: 1px solid #000;">PRUEBAS DE LIBERACIÓN</td>
        </tr>
        <tr style="background-color: #9BD2E3; text-align: center;">
            <td style="border: 1px solid #000; width: 40%;">&nbsp;</td>
            <td style="border: 1px solid #000; width: 20%;">INICIO CORRIDA</td>
            <td style="border: 1px solid #000; width: 20%;">MEDIO CORRIDA</td>
            <td style="border: 1px solid #000; width: 20%;">FIN CORRIDA</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="border: 1px dotted #000; padding-left: 5px;">@item.ControlOperationName</td>
                <td style="border: 1px dotted #000; text-align: center;">@item.InicioCorrida</td>
                <td style="border: 1px dotted #000; text-align: center;">@item.MedioCorrida</td>
                <td style="border: 1px dotted #000; text-align: center;">@item.FinCorrida</td>
            </tr>
        }
    </tbody>
</table>
