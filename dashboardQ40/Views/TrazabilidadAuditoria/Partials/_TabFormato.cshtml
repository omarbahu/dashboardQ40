<!-- Tratamiento de Agua -->
<div class="accordion" id="accordion-Agua">
    <div class="accordion-item">
        <h2 class="accordion-header">
            <button class="accordion-button py-1" type="button" data-bs-toggle="collapse" data-bs-target="#collapseAgua">
                <h4 class="mb-0">@Localizer["Water_Header"]</h4>
            </button>
        </h2>
        <div id="collapseAgua" class="accordion-collapse collapse show">
            <div class="accordion-body">

                <!-- 1. General -->
                @await Html.PartialAsync("~/Views/TrazabilidadAuditoria/Partials/_F_JarabesLoteJarabeSimple.cshtml", Model.BloqueAguaTratada)

                <!-- 2. Fisicoquímicos -->
                @await Html.PartialAsync("~/Views/TrazabilidadAuditoria/Partials/_F_JarabesLoteJarabeSimpleFisicoquimico.cshtml", Model.Bloqueaguatratadajarabesimple)

                <!-- 3. Agua tratada usada en jarabe simple -->
                @await Html.PartialAsync("~/Views/TrazabilidadAuditoria/Partials/_F_JarabesLoteJarabeSimpleFisicoquimico.cshtml", Model.Bloqueaguatratadajarabesimple)

                <!-- 4. Agua tratada usada en jarabe termido -->
                @await Html.PartialAsync("~/Views/TrazabilidadAuditoria/Partials/_F_JarabesLoteJarabeSimpleFisicoquimico.cshtml", Model.Bloqueaguatratadajarabeterminado)

                <!-- 5. PT + sin recuperada -->
                @await Html.PartialAsync("~/Views/TrazabilidadAuditoria/Partials/_F_JarabesLoteJarabeSimpleFisicoquimico.cshtml", Model.Bloqueaguatratadacruda)

                <!-- 6. Agua suave sin queja -->
                @await Html.PartialAsync("~/Views/TrazabilidadAuditoria/Partials/_F_JarabesLoteJarabeSimpleFisicoquimico.cshtml", Model.Bloqueaguatratadasuave)

                <!-- 7. Sensorial tratada -->
